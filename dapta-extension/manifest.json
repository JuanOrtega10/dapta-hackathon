{
  "manifest_version": 3,
  "name": "Dapta Agent Exporter",
  "version": "1.6.0",
  "description": "Export Dapta voice agent information to external services",
  "permissions": ["activeTab"],
  "host_permissions": ["https://app.dapta.ai/*", "https://api.dapta.ai/*"],
  "content_scripts": [
    {
      "matches": ["https://app.dapta.ai/agents-studio/voice-agents/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
